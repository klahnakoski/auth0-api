{
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1,
//		"ssl_context": "adhoc",
		"load_dotenv": false
	},
	"session": {
		"db": {
			"filename": "sessions.sqlite"
		},
		"table": "sessions",
		"use_signer": false,
		"cookie": {
			"name":"annotation_session",
			"domain": "dev.localhost",
			"path": "/",
			"secure": false,
			"lifetime": 31536000,
			"httponly": false
		}
	},
	"permissions": {
		"store": "permissions.sqlite"
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "auth0-api"
		}
	},
	"auth0": {
		"domain": "dev-8821kz09.auth0.com",
		"api.identifier": "https://locahost/query",
		"scope": "query:send"
	},
	"debug": {
		"trace": true,
		"log": [
			{
				"log_type": "console"
			}
		]
	}
}
